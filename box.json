{   "id": "com.sample.box",
    "version": "1.0.0",
    "name": "Box",
    "description": "A colored box",
    "newInstancePrefix": "Box",
    "icon": "https://3aa4-2003-f3-3f23-24f9-a409-d98a-6c18-68ef.ngrok.io/static/icon.png",
    "vendor": "Sample",
    "eula": "",
    "license": "",
    "webcomponents": [{
            "kind": "main",
            "tag": "com-sample-box",
            "url": "https://3aa4-2003-f3-3f23-24f9-a409-d98a-6c18-68ef.ngrok.io/static/box.js",
            "integrity": "",
            "ignoreIntegrity": true
        },
        {
            "kind": "styling",
            "tag": "com-sample-box-styling",
            "url": "https://3aa4-2003-f3-3f23-24f9-a409-d98a-6c18-68ef.ngrok.io/static/box_styling.js",
            "integrity": "",
            "ignoreIntegrity": true
        },
        {
            "kind": "builder",
            "tag": "com-sample-box-builder",
            "url": "https://3aa4-2003-f3-3f23-24f9-a409-d98a-6c18-68ef.ngrok.io/static/box_builder.js",
            "integrity": "",
            "ignoreIntegrity": true
        }
    ],
    "properties": {
        "color": {
            "type": "string",
            "description": "Background color",
            "default": "red"
        },
        "opacity": {
            "type": "number",
            "description": "Opacity",
            "default": 1
        },
        "width": {
            "type": "integer",
            "default": 100
        },
        "height": {
            "type": "integer",
            "default": 100
        }
    },
    "methods": {
        "setColor": {
            "description": "Sets the background color.",
            "parameters": [{
                "name": "newColor",
                "type": "string",
                "description": "The new background color"
            }],
            "body": "this.color = newColor;"
        },
        "getColor": {
            "returnType": "string",
            "description": "Returns the background color.",
            "body": "return this.color;"
        }
    },
    "events": {
        "onClick": {
            "description": "Called when the user clicks the Colored Box."
        }
    }
}